{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "/private/var/folders/64/1ydwh2_x7pnfstlcr9csc3m40000gn/T/AppTranslocation/8FA72803-6970-474D-95AF-2487B680E648/d/Godot_mono.app/Contents/MacOS/Godot",
            "type": "process",
            "args": [
                "--build-solutions",
                "--path",
                "${workspaceRoot}",
                "--no-window",
                "-q"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Poker-Game.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/Poker-Game.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}